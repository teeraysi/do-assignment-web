{
  "compileOnSave": false,
  "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "allowJs": true,
      "jsx": "preserve",
      "outDir": "dist",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false,
      "strict": false,
      "strictNullChecks": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "preserveConstEnums": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "lib": [
          "dom",
          "es2015",
          "es2016",
          "es2017"
      ],
      "baseUrl": ".",
      "typeRoots": [
          "./node_modules/@types"
      ],
      "paths": {
          "i18n": [
              "./i18n.ts"
          ],
          "api/*": [
              "./src/api/*"
          ],
          "definitions/*": [
              "./src/definitions/*"
          ],
          "common/*": [
              "./src/common/*"
          ],
          "models/*": [
              "./src/models/*"
          ],
          "modules/*": [
              "./src/modules/*"
          ],
          "stores": [
              "./src/store/index.ts"
          ],
          "store/*": [
              "./src/store/*"
          ],
          "utils/*": [
              "./src/utils/*"
          ]
      }
  },
  "includes": [
      "./src/definitions/externals.d.ts",
      "next-env.d.ts"
  ]
}